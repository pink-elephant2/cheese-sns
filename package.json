{
  "name": "cheese-sns",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --prod --proxy-config proxy.conf.json --ssl true",
    "start:local": "ng serve --configuration=stg --proxy-config proxy.conf.local.json",
    "build": "ng build --prod --aot=true --base-href=/cheese-sns/",
    "build:local": "ng build --configuration=stg --aot=true --base-href=/cheese-sns/",
    "build:gh": "ng build --prod --aot=true --base-href=/cheese-sns/ && angular-cli-ghpages -d dist/cheese-sns",
    "test": "ng test",
    "lint": "ng lint --type-check",
    "sasslint": "sass-lint -c .sass-lint.yml -q -v",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.7",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^8.2.7",
    "@angular/compiler": "^8.2.7",
    "@angular/core": "^8.2.7",
    "@angular/forms": "^8.2.7",
    "@angular/platform-browser": "^8.2.7",
    "@angular/platform-browser-dynamic": "^8.2.7",
    "@angular/router": "^8.2.7",
    "animate.css": "^3.7.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.5",
    "ng2-adsense": "^5.4.2",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "url-search-params-polyfill": "^6.0.0",
